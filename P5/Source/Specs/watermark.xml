<!ELEMENT heraldry - - (%phrase.seq;)>-->
    <rng:ref name="macro.phraseSeq"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <support>
        <p><material>Rag paper</material> with <watermark>anchor</watermark> watermark</p>
      </support>
    </egXML>
  </exemplum>
  <listRef>
    <ptr target="#mswat"/>
  </listRef>
</elementSpec>
