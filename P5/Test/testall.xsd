  <xs:attributeGroup name="att.timed.attribute.start">
    <xs:attribute name="start" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>indicates the location within a temporal alignment at which this element begins.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
