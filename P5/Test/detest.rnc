           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "detest-dataRef-restrictAttRestriction-constraint-rule-29"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:dataRef[tei:dataFacet]"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@restriction"
             role = "nonfatal"
             "The attribute restriction cannot be used when dataFacet elements are present."
