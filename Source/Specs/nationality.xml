  <exemplum xml:lang="en">
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <nationality key="US" notBefore="1966"> Obtained US Citizenship in 1966</nationality>
    </egXML>
  </exemplum>
  <exemplum version="2008-04-06" xml:lang="fr">
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
