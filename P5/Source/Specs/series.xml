<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="core" xml:id="SERIES" usage="mwa" ident="series">
  <equiv/>
  <gloss>series information</gloss>
  <gloss version="2007-01-21" xml:lang="it">informazioni sulla collana</gloss>
  <gloss version="2007-05-04" xml:lang="es">series de información</gloss>
  <gloss version="2006-06-05" xml:lang="zh-tw">叢書資訊</gloss>
  <gloss version="2006-10-28" xml:lang="ja">シリーズ情報</gloss>
  <desc>contains information about the series in which a book or other
bibliographic item has appeared.</desc>
  <desc version="2007-01-21" xml:lang="it">contiene indicazioni sulla collana in cui un libro o un'altra unità bibliografica sono stati pubblicati.</desc>
  <desc version="2007-05-04" xml:lang="es">contiene información sobre las series en que un libro u otro elemento bibliográfico ha aparecido.</desc>
  <desc version="2006-06-05" xml:lang="zh-tw">包含某書目項目所屬的叢書資訊。</desc>
  <desc version="2006-10-28" xml:lang="ja"> 書籍などの書誌情報項目がシリーズとしてあるものにつ
いての情報．</desc>
  <classes>
    <memberOf key="model.biblPart"/>
  </classes>
  <content>
    <rng:zeroOrMore xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <rng:choice>
        <rng:text/>
        <rng:ref name="model.gLike"/>
        <rng:ref name="title"/>
        <rng:ref name="editor"/>
        <rng:ref name="respStmt"/>
        <rng:ref name="biblScope"/>
        <rng:ref name="model.global"/>
      </rng:choice>
    </rng:zeroOrMore>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
  </exemplum>
  <remarks>
    <p/>
  </remarks>
  <listRef>
    <ptr target="#COBICOL"/>
  </listRef>
</elementSpec>
