<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="namesdates" xml:id="MONTH" usage="opt" ident="month">
<equiv/>
<gloss>month</gloss>
<desc>the month component of a structured date.</desc>
  <classes>
    <memberOf key="model.datePart"/>
    <memberOf key="att.datePart"/>
  </classes>
  <content>
    <rng:ref name="macro.xtext" xmlns:rng="http://relaxng.org/ns/structure/1.0"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">      <date value="1993-05-14"><day type="name">Friday</day>,
<day type="number">14</day><month>May</month><year>1993</year></date>
    </egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#NDDATE"/>
  </listRef>
</elementSpec>
