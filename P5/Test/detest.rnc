    attribute ident { xsd:Name },
    
    ## supplies a version number for the encoding conventions used, if any.
    attribute version {
      xsd:token {
        pattern = "[\d]+[a-z]*[\d]*(\.[\d]+[a-z]*[\d]*){0,3}"
      }
    }?,
    empty
