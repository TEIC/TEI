      <principal xml:id="fr_AEU-2">Anne-Élisabeth Urfels-Capot</principal>
    </egXML>
  </exemplum>
  <exemplum xml:lang="zh-TW">
    <egXML xmlns="http://www.tei-c.org/ns/Examples" xml:id="gi-principal-egXML-yz">
      <principal>蔡元培</principal>
    </egXML>
  </exemplum>
