    (text
     | model.gLike
     | model.castItemPart
     | model.phrase
     | model.global)*,
    att.global.attributes,
    
    ## characterizes the cast item.
    ##    
    [ a:defaultValue = "role" ]
    attribute type {
      
      ## the item describes a single role.
      ##        
      "role"
      | 
        ## the item describes a list of non-speaking roles.
        ##    
        "list"
    }?,
    empty
