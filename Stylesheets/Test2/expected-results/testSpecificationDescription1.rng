                           <ref name="textLang"/>
                           <ref name="editor"/>
                           <ref name="respStmt"/>
                        </choice>
                     </zeroOrMore>
            
                  </group>
                  <group>
            
                     <oneOrMore>
                        <choice>
                           <ref name="title"/>
                           <ref name="model.ptrLike"/>
                           <ref name="idno"/>
                        </choice>
                     </oneOrMore>
            
            
                     <zeroOrMore>
