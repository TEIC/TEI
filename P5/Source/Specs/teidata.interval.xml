<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?><dataSpec xmlns="http://www.tei-c.org/ns/1.0"
          xmlns:tei="http://www.tei-c.org/ns/1.0"
          module="tei"
          ident="teidata.interval">
  <desc versionDate="2013-04-13"
         xml:lang="en">defines attribute values used to express an interval value.</desc>
  <content>
      <alternate>
         <dataRef name="float"/>
         <valList>
            <valItem ident="regular"/>
            <valItem ident="irregular"/>
            <valItem ident="unknown"/>
         </valList>
      </alternate>
   </content>
  <remarks versionDate="2013-04-13"
            xml:lang="en">
      <p>Any value greater than zero or any one of the values
    <val>regular</val>, <val>irregular</val>, <val>unknown</val>.</p>
  </remarks>
</dataSpec>
