      <ref name="att.transcriptional.attributes"/>
      <ref name="att.typed.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="modSpan">
    <element name="modSpan" ns="http://www.tei-c.org/ns/geneticEditions">
