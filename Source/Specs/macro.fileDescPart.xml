<macroSpec xmlns="http://www.tei-c.org/ns/1.0" xml:id="MCFDP" module="tei" type="pe" ident="macro.fileDescPart">
  <gloss>file description elements</gloss>
  <desc>groups elements which occur inside <gi>fileDesc</gi> and <gi>biblFull</gi></desc>
  <content xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns="http://www.tei-c.org/ns/1.0">
    <rng:group>
      <rng:ref name="titleStmt"/>
      <rng:optional>
	<rng:ref name="editionStmt"/>
      </rng:optional>
      <rng:optional>
	<rng:ref name="extent"/>
      </rng:optional>
      <rng:ref name="publicationStmt"/>
      <rng:optional>
	<rng:ref name="seriesStmt"/>
      </rng:optional>
      <rng:optional>
	<rng:ref name="notesStmt"/>
      </rng:optional>
    </rng:group>
  </content>
  <listRef>
    <ptr target="#HD2"/>
    <ptr target="#COBITY"/>
  </listRef>
</macroSpec>
