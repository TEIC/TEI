   <text>
      <body>
         <div>
            <head/>
            <div xml:id="GoBack">
               <head>Simple ordered</head>
               <list type="ordered">
                  <item>Cats</item>
                  <item>Bats</item>
                  <item>Flats</item>
               </list>
               <p>Done</p>
            </div>
            <div>
               <head>Simple unordered</head>
               <list type="unordered">
                  <item>Cats</item>
                  <item>Bats</item>
                  <item>Flats</item>
               </list>
               <p>Done</p>
            </div>
            <div>
               <head>Nested and ordered</head>
               <list type="ordered">
                  <item>Cats</item>
                  <item>Dogs<list type="ordered">
                        <item>Alsatians</item>
                        <item>Greyhounds</item>
                     </list>
                  </item>
                  <item>Sheep</item>
               </list>
               <p>done</p>
            </div>
            <div>
               <head>Nested and unordered</head>
               <list type="unordered">
                  <item>Cats</item>
                  <item>Dogs<list type="unordered">
                        <item>Alsatians</item>
                        <item>Greyhounds</item>
                     </list>
                  </item>
                  <item>sheep</item>
               </list>
               <p>done</p>
            </div>
            <div>
               <head>Now insert a List Number</head>
               <list type="ordered">
                  <item>Cats</item>
                  <item>Bats</item>
                  <item>Flats</item>
               </list>
            </div>
            <div>
               <head>List Number 2</head>
               <list type="ordered">
                  <item>Cats</item>
                  <item>Bats</item>
                  <item>Flats</item>
               </list>
            </div>
            <div>
               <head>List Bullet</head>
               <list type="unordered">
                  <item>Cats</item>
                  <item>Bats</item>
                  <item>Flats</item>
               </list>
            </div>
         </div>
      </body>
