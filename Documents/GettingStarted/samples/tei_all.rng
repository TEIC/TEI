        <rng:list>
          <rng:oneOrMore>
            <data type="Name"/>
          </rng:oneOrMore>
        </rng:list>
      </rng:attribute>
    </rng:optional>
  </define>
  <define xmlns="http://relaxng.org/ns/structure/1.0" name="att.handFeatures.attribute.medium">
