<moduleSpec xmlns="http://www.tei-c.org/ns/1.0" ident="gaiji">
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="g" module="gaiji">

<gloss>character or glyph</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>represents a non-standard character or glyph.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
  
  
  <attList>
    <attDef ident="ref">
	
	<desc>points to a description of the  character or glyph intended.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
      
      <valDesc>a pointer to some another element.</valDesc><valDesc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</valDesc>
    </attDef>
</attList>
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="char" module="gaiji">

<gloss>character</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>provides descriptive information about a character. </desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>


  

</elementSpec>
  <elementSpec ident="charName" module="gaiji">

<gloss>character name</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains the name of a character, expressed following Unicode conventions. </desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>


  

</elementSpec>
  <elementSpec ident="charProp" module="gaiji">

<gloss>character property</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>provides a name and value for some property of the parent
character or glyph. </desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>


  


</elementSpec>
  <elementSpec ident="charDesc" module="gaiji">

<gloss>character description</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>provides descriptive information about characters or
glyphs. </desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>


  

</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="glyph" module="gaiji">

<gloss>character glyph</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>provides descriptive information about a character glyph. </desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>


  


</elementSpec>
  <elementSpec ident="glyphName" module="gaiji">

<gloss>character glyph name</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains the name of a glyph, expressed following Unicode
conventions for character names. </desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>


  

 </elementSpec>
  <elementSpec ident="localName" module="gaiji">

<gloss>Locally-defined Property Name</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains a locally defined name for some property. </desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>


  


</elementSpec>
  <elementSpec ident="mapping" module="gaiji">

<gloss>character mapping</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains one or more
	 characters which are related to the parent character or glyph
	 in some respect, as specified by the <att>type</att>
	 attribute.
</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>


  

</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="unicodeName" module="gaiji">

<gloss>Unicode Property Name</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains the name of a registered Unicode normative or
informative property. </desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>


  <attList>
<attDef ident="version" usage="opt">
	
	<desc>specifies the version number of the Unicode Standard in
        which this property name is defined.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
      
      <valDesc>a valid version number.</valDesc><valDesc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</valDesc>
    </attDef>
</attList>


</elementSpec>
  <elementSpec ident="value" module="gaiji">

<gloss>value</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains a single value for some property, attribute, or other
  analysis.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
  
  
  
  
</elementSpec>
</moduleSpec>
