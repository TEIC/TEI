         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "detest-damageSpan-spanTo_fr-constraint-assert-15"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:damageSpan"
           "\x{a}" ~
           "               "
           sch:assert [
