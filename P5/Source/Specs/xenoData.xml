	<dc:subject>British Columbia</dc:subject>
	<dc:description>British Columbia correspondence: Public Offices
	document (normally correspondence between government
	departments)</dc:description>
      </oai_dc:dc>
    </xenoData>
    </egXML>
  </exemplum>
