<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<macroSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" module="tei" type="dt" ident="data.interval" validUntil="2018-10-01">
  <desc versionDate="2013-04-13" xml:lang="en">defines attribute values used to express an interval value.</desc>
    <desc type="deprecationInfo" xml:lang="en"
	  versionDate="2018-09-14">Use <name
	  type="datatype">teidata.interval.xml</name> instead.</desc>
  <content>
    <choice xmlns="http://relaxng.org/ns/structure/1.0">
      <data type="float">
        <param name="minExclusive">0</param>
      </data>
      <value>regular</value>
      <value>irregular</value>
      <value>unknown</value>
    </choice>
  </content>
  <remarks versionDate="2013-04-13" xml:lang="en">
    <p>Any value greater than zero or any one of the values
    <val>regular</val>, <val>irregular</val>, <val>unknown</val>.</p>
  </remarks>
</macroSpec>
