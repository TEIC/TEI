<moduleSpec xmlns="http://www.tei-c.org/ns/1.0" ident="dictionaries">
  <classSpec ident="model.entryLike" module="dictionaries">
<desc>groups those component-level elements which are unique to the base tag set for dictionaries.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
</classSpec>
  <macroSpec ident="mix.dictionaries" module="dictionaries">
<gloss>mixed-base dictionary components</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains a string used in constructing the definition of<term>macro.component</term> used
        in the mixed base tag set.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
</macroSpec>
  <classSpec ident="att.entryLike" module="dictionaries">
<gloss>dictionary entries</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>groups the different styles of dictionary entries.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    <attList>
        <attDef ident="type" usage="rwa">
	
	<desc>indicates type of entry, in dictionaries with multiple types.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            
            <valList type="semi">
                <valItem ident="main">
                    
                    <gloss>a main entry (default).</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="hom">
                    
                    <gloss>a homograph with a separate entry.</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="xref">
                    
                    <gloss>a reduced entry whose only function is to point to another main entry
                        (e.g. for forms of an irregular verb or for variant spellings:
                        <mentioned>was</mentioned> pointing to <mentioned>be</mentioned>, or
                        <mentioned>esthete</mentioned> to <mentioned>aesthete</mentioned>).</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="affix">
                    
                    <gloss>an entry for a prefix, infix, or suffix.</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="abbr">
                    
                    <gloss>an entry for an abbreviation.</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="supplemental">
                    
                    <gloss>a supplemental entry (for use in dictionaries which issue supplements to
                        their main work in which they include updated information about entries).</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="foreign">
                    
                    <gloss>an entry for a foreign word in a monolingual dictionary.</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
            </valList>
        </attDef>
        <attDef ident="sortKey" usage="opt">
	
	<desc>contains a (sortable) character sequence reflecting the entry's alphabetical
                position in the printed dictionary.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            <valDesc>any sequence of characters which, when sorted with the other values, will
                produced the desired order; specifics of sort key construction are application-dependent.</valDesc><valDesc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</valDesc>
            
        </attDef>
    </attList>
    
    
</classSpec>
  <elementSpec ident="superEntry" module="dictionaries">

<gloss/><gloss xml:lang="zh-tw" notBefore="2007-05-02"/>
<desc>groups successive entries for a set of homographs.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="entry" module="dictionaries">

<gloss/><gloss xml:lang="zh-tw" notBefore="2007-05-02"/>
<desc>contains a reasonably well-structured dictionary entry.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
</elementSpec>
  <elementSpec ident="entryFree" module="dictionaries">
    
    <gloss/><gloss xml:lang="zh-tw" notBefore="2007-05-02"/>
    <desc>contains a dictionary entry which does not necessarily conform to the constraints imposed
        by the <gi>entry</gi> element.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
    
    
</elementSpec>
  <elementSpec ident="hom" module="dictionaries">

<gloss>homograph</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>groups information relating to one homograph within an entry.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="sense" module="dictionaries">

<gloss>sense information group</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>groups together all information relating to one word sense in a dictionary entry
        (definitions, examples, translation equivalents, etc.)</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
  
  
  <attList>
    <attDef ident="level" usage="opt">
	
	<desc>gives the nesting depth of this sense.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
      
      <valDesc>a positive integer</valDesc><valDesc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="dictScrap" module="dictionaries">

<gloss/><gloss xml:lang="zh-tw" notBefore="2007-05-02"/>
<desc>encloses a part of a dictionary entry in which other phrase-level dictionary elements are
        freely combined.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="form" module="dictionaries">

<gloss>form information group</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>groups all the information on the written and spoken forms of one headword.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    <attList>
        <attDef ident="type" usage="opt">
	
	<desc>classifies form as simple, compound, etc.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            <valList type="semi">
                <valItem ident="simple">
                    
                    <gloss>single free lexical item</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="lemma">
                    
                    <gloss>the headword itself</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="variant">
                    
                    <gloss>a variant form</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="compound">
                    
                    <gloss>word formed from simple lexical items</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="derivative">
                    
                    <gloss>word derived from headword</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="inflected">
                    
                    <gloss>word in other than usual dictionary form</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="phrase">
                    
                    <gloss>multiple-word lexical item</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
            </valList>
            
        </attDef>
    </attList>
    
    
    
</elementSpec>
  <elementSpec ident="orth" module="dictionaries">

<gloss>orthographic form</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>gives the orthographic form of a dictionary headword.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    <attList>
        <attDef ident="type" usage="opt">
	
	<desc>gives the type of spelling.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            <valDesc>Any convenient word or phrase, e.g. <mentioned>lat</mentioned>
                (latinate),<mentioned>std</mentioned> (standard), <mentioned>trans</mentioned>
                (transliterated), etc.</valDesc><valDesc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</valDesc>
            
            
        </attDef>
        <attDef ident="extent" usage="opt">
	
	<desc>gives the extent of the orthographic information provided.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            
            <valList type="open">
                <valItem ident="full">
                    
                    <gloss>full form</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="pref">
                    
                    <gloss>prefix</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="suff">
                    
                    <gloss>suffix</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="part">
                    
                    <gloss>partial</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
            </valList>
            
            
        </attDef>
    </attList>
    
    
    
</elementSpec>
  <elementSpec ident="pron" module="dictionaries">

<gloss>pronunciation</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains the pronunciation(s) of the word.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    <attList>
        <attDef ident="extent" usage="opt">
	
	<desc>indicates whether the pronunciation is for whole word or part. </desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            
            <valList type="open">
                <valItem ident="full">
                    
                    <gloss>full form</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="pref">
                    
                    <gloss>prefix</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="suff">
                    
                    <gloss>suffix</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="part">
                    
                    <gloss>partial</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
            </valList>
            
            
        </attDef>
        <attDef ident="notation" usage="rwa">
	
	<desc>indicates what notation is used for the pronunciation, if more than one occurs in
                the machine-readable dictionary. </desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            <valDesc>Sample values: IPA, Murray, ... </valDesc><valDesc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</valDesc>
            
            
        </attDef>
    </attList>
    
    
    
</elementSpec>
  <elementSpec ident="hyph" module="dictionaries">

<gloss>hyphenation</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains a hyphenated form of a dictionary headword, or hyphenation information in some
        other form.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="syll" module="dictionaries">

<gloss>syllabification</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains the syllabification of the headword.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="stress" module="dictionaries">

<gloss/><gloss xml:lang="zh-tw" notBefore="2007-05-02"/>
<desc>contains the stress pattern for a dictionary headword, if given separately.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="gram" module="dictionaries">

<gloss>grammatical information</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>within an entry in a dictionary or a terminological data file, contains grammatical
        information relating to a term, word, or form.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    <attList>
        <attDef ident="type" usage="opt">
	
	<desc>classifies the grammatical information given according to some convenient typology
                â€” in the case of terminological information, preferably the dictionary of
                data element types specified in ISO WD 12 620.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            <valList type="open">
                <valItem ident="pos">
                    
                    <gloss>part of speech (any of the word classes to which a word may be assigned
                        in a given language, based on form, meaning, or a combination of features,
                        e.g. noun, verb, adjective, etc.)</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="gen">
                    
                    <gloss>gender (formal classification by which nouns and pronouns, and often
                        accompanying modifiers, are grouped and inflected, or changed in form, so as
                        to control certain syntactic relationships)</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="num">
                    
                    <gloss>number (e.g. singular, plural, dual, ...)</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="animate">
                    
                    <gloss>animate or inanimate</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="proper">
                    
                    <gloss>proper noun or common noun</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
            </valList>
            
            
        </attDef>
    </attList>
    
    
    
</elementSpec>
  <elementSpec ident="gen" module="dictionaries">

<gloss>gender</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>identifies the morphological gender of a lexical item, as given in the dictionary.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="number" module="dictionaries">

<gloss/><gloss xml:lang="zh-tw" notBefore="2007-05-02"/>
<desc>indicates grammatical number associated with a form, as given in a dictionary.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="case" module="dictionaries">

<gloss/><gloss xml:lang="zh-tw" notBefore="2007-05-02"/>
<desc>contains grammatical case information given by a dictionary for a given form.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="per" module="dictionaries">

<gloss>person</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains an indication of the grammatical person (1st, 2nd, 3rd, etc.) associated with a
        given inflected form in a dictionary.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="tns" module="dictionaries">

<gloss>tense</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>indicates the grammatical tense associated with a given inflected form in a dictionary.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="mood" module="dictionaries">

<gloss/><gloss xml:lang="zh-tw" notBefore="2007-05-02"/>
<desc>contains information about the grammatical mood of verbs (e.g. indicative, subjunctive, imperative)</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="iType" module="dictionaries">

<gloss>inflectional class</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>indicates the inflectional class associated with a lexical item.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    <attList>
        <attDef ident="type" usage="opt">
	
	<desc>indicates the type of indicator used to specify the inflection class, when it is
                necessary to distinguish between the usual abbreviated indications (e.g.
                <mentioned>inv</mentioned>) and other kinds of indicators, such as special codes
                referring to conjugation patterns, etc.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            <valList type="open">
                <valItem ident="abbrev">
                    
                    <gloss>abbreviated indicator</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="verbTable">
                    
                    <gloss>coded reference to a table of verbs</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
            </valList>
            
            
        </attDef>
    </attList>
    
    
    
</elementSpec>
  <classSpec ident="model.morphLike" module="dictionaries">
<gloss>morphological elements</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>groups elements which provide morphological information within the dictionary tag set.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
</classSpec>
  <classSpec ident="model.formPart" module="dictionaries">
<gloss>form information</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>groups elements allowed within a <gi>form</gi> element in a dictionary.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
</classSpec>
  <elementSpec ident="gramGrp" module="dictionaries">

<gloss>grammatical information group</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>groups morpho-syntactic information about a lexical item, e.g. <gi>pos</gi>, <gi>gen</gi>, <gi>number</gi>, <gi>case</gi>, or
        <gi>iType</gi> (inflectional class).</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="pos" module="dictionaries">

<gloss>part of speech</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>indicates the part of speech assigned to a dictionary headword (noun, verb, adjective, etc.)</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="subc" module="dictionaries">

<gloss>subcategorization</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains subcategorization information (transitive/intransitive, countable/non-countable, etc.)</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="colloc" module="dictionaries">

<gloss>collocate</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains a collocate of the headword.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    <attList>
        <attDef ident="type" usage="opt">
	
	<desc>classifies the collocation, using any convenient typology.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            <valDesc>any string of XML name characters, e.g. <mentioned>preposition</mentioned>.</valDesc><valDesc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</valDesc>
            
            
        </attDef>
    </attList>
    
    
    
</elementSpec>
  <classSpec ident="model.gramPart" module="dictionaries">
<gloss>grammatical information</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>groups those elements allowed within a <gi>gramGrp</gi> element in a dictionary.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
</classSpec>
  <elementSpec ident="def" module="dictionaries">

<gloss>definition</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains definition text in a dictionary entry.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="tr" module="dictionaries">

<gloss>translation equivalent</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains a translation of the headword or an example.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="trans" module="dictionaries">

<gloss>translation information</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains translation text and related information (within an entry in a multilingual dictionary).</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="etym" module="dictionaries">

<gloss>etymology</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>encloses the etymological information in a dictionary entry.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="lang" module="dictionaries">
  
  <gloss>language name</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
  <desc>name of a language mentioned in etymological or other linguistic discussion.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
  
  
  
  
  
</elementSpec>
  <elementSpec ident="dicteg" module="dictionaries">

<gloss>example (exempli gratia)</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>(in a dictionary) contains an example text containing at least one occurrence of the word
        form, used in the sense being described; examples may be quoted from (named) authors or contrived.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    
    
    
</elementSpec>
  <elementSpec ident="usg" module="dictionaries">

<gloss>usage</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
<desc>contains usage information in a dictionary entry.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
    
    
    <attList>
        <attDef ident="type" usage="opt">
	
	<desc>classifies the usage information using any convenient typology.</desc><desc xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</desc>
            
            <valList type="open">
                <valItem ident="geo">
                    
                    <gloss>geographic area</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="time">
                    
                    <gloss>temporal, historical era (archaic, old, etc.)</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="dom">
                    
                    <gloss>domain</gloss><gloss xml:lang="zh-tw" notBefore="2007-05-02">TO BE TRANSLATED</gloss>
                </valItem>
                <valItem ident="reg">
                    
